    coffeescript ->
      colors = [
        '#ff0000'
        '#ff3000'
        '#ff5f00'
        '#ff9000'
        '#ffbf00'
        '#fff000'
        '#dfff00'
        '#aeff00'
        '#7fff00'
        '#4fff00'
        '#1fff00'
        '#00ff11'
        '#00ff41'
        '#00ff72'
        '#00ffa1'
        '#00ffd1'
        '#00fdff'
        '#00cdff'
        '#009dff'
        '#006dff'
        '#003dff'
        '#000dff'
        '#2300ff'
        '#5300ff'
        '#8300ff'
        '#b200ff'
        '#e200ff'
        '#ff00eb'
        '#ff00bb'
        '#ff008c'
        '#ff005b'
        '#ff002c'
      ]

      $ ->
        button = $('.buy button')
        text = button.text()
        button.html('')
        for letter in text
          if letter is ' '
            letter = '&nbsp;'
          button.append '<span class="letter">'+letter+'</span>'
        interval = 0
        offset = 0
        colorz = ->
          chillin = button.children()
          i = chillin.length
          for child in chillin
            $(child).css 'color', colors[(offset+i)%colors.length]
            $(child).css '-webkit-transform', "translateZ(#{20*Math.sin((offset+i)*0.1)}px)"
            $(child).css '-moz-transform', "translateZ(#{20*Math.sin((offset+i)*0.1)}px)"
            $(child).css 'transform', "translateZ(#{20*Math.sin((offset+i)*0.1)}px)"
            # $(child).removeClass 's'+((offset+i-1)%57)
            # $(child).addClass 's'+((offset+i)%57)
            --i
          ++offset
          interval = requestAnimationFrame colorz
        button.mouseover ->
          colorz()

        button.mouseout ->
          cancelAnimationFrame interval
          button.children().css 'color', 'inherit'
          button.children().css '-webkit-transform', "translateZ(0px)"
          button.children().css '-moz-transform', "translateZ(0px)"
          button.children().css 'transform', "translateZ(0px)"

